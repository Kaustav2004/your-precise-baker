{
  "version": 2,
  "builds": [{
    "src": "package.json",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "dist",
      "cleanUrls": true
    }
  }],
  "routes": [
    {
      "src": "/(auth/)?assets/.*",
      "status": 404,
      "headers": { "Cache-Control": "no-cache" }
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/auth/callback",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}